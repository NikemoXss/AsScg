<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="publicloadingProgressStyle">
        <item name="android:maxWidth">108dip</item>
        <item name="android:maxHeight">104dip</item>
        <item name="android:indeterminateDrawable">@drawable/wb_loading_frame</item>
        <item name="android:minWidth">108dip</item>
        <item name="android:minHeight">104dip</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="title_img">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="white20Text">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="zdwz10Text">
        <item name="android:textColor">@color/zdwz</item>
        <item name="android:textSize">10sp</item>
    </style>


    <style name="gray10Text">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="red25Text">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">25sp</item>
    </style>

    <style name="red10Text">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="tab_bar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">-12dp</item>
        <item name="android:textColor">@drawable/tab_color_selector</item>
        <item name="android:button">@null</item>
    </style>

    <style name="progressBarHorizontal_color" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_color_horizontal</item>
        <item name="android:minHeight">5dip</item>
        <item name="android:maxHeight">5dip</item>
    </style>

    <style name="black20Text">
        <item name="android:textColor">@color/title_black</item>
        <item name="android:textSize">20sp</item>
    </style>


    <style name="black13Text">
        <item name="android:textColor">@color/title_black</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="black18Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="black16Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="orange16Text">
        <item name="android:textColor">@color/orgine</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="red16Text">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="red18Text">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="white18Text">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Style_Dialog_Button">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ff425968</item>
        <item name="android:background">@drawable/btn_default</item>
        <item name="android:layout_marginLeft">6.0dip</item>
        <item name="android:layout_marginRight">6.0dip</item>
        <item name="android:minHeight">40.0dip</item>
    </style>

    <style name="blue18Text">
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="gray16Text">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="edittext" parent="@android:style/Widget.EditText">
        <item name="android:background">#ffffff</item>
    </style>

    <style name="gray18Text">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="gray13Text">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">13sp</item>
    </style>
</resources>
